<h1>Sign In</h1>

<%= form_with url: '/users/sign_in', method: :post do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <%= form.submit %>
<% end %>